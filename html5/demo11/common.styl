*
    margin 0
    padding 0
    ul,li
        list-style none

html
    color #fff
    background-color #ffffff

.section
    margin-bottom 30px
    min-height 200

.wrapper
    width 950px
    height 100px
    background-color #112233
    margin 0 auto

.sidenav
    float left
    width 100px
    margin-right 20px
    h2
        color #072
        margin 0 0 12px 0
        line-height 150%
        font 15px
        a
            font-size 24px
            text-decoration none

    .side-links
        margin-top 40px
        li
            margin-top 5px
            font-style 14px
            a
                color #2277aa
                &:hover
                    color #669

